for(i=0;i<n;i++){
        wt[i]=0;
        tat[i]=0;
        temp[i+1]=temp[i]+bt[i];
        wt[i]=temp[i]-at[i];
        tat[i]=wt[i]+bt[i];
        awt+=wt[i];
        atat+=tat[i];
        printf("%d%d%d%d%d\n",i+1,at[i],bt[i],wt[i],tat[i]);
}
awt=awt/n;
atat=atat/n;
printf("\nAverage Waiting Time = %f\n",awt);
printf("Average Turn Around Time = %f",atat);
